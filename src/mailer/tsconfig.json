{
  "compilerOptions": {
    "outDir": "wwwroot/app",
    "module": "commonjs",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "suppressImplicitAnyIndexErrors": true,
    "noImplicitAny": false,
    "removeComments": false,
    "sourceMap": true,
    "target": "ES5"
  },
  "exclude": [
    "node_modules",
    "typings/main",
    "typings/main.d.ts"
  ]
}
